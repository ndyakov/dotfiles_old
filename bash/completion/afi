# afi (apt-fast install) completion                              -*- shell-script -*-

_afi()
{
    local cur prev words cword
    _init_completion || return

    local special i
    COMPREPLY=( $( apt-cache --no-generate pkgnames "$cur" \
        2> /dev/null ) )
    return 0
} &&
complete -F _afi afi

# ex: ts=4 sw=4 et filetype=sh
